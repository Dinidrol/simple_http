{
    "configurations": [
        {
            "name": "Generic",
            "includePath": [
                "${workspaceFolder}/main/**",
                "${workspaceFolder}/build/include/",
                "${env.HOME}/esp/esp-idf/components/**",
                "${env.HOME}/esp/esp-idf-lib/components/**",
                "${workspaceFolder}/components/espfs/include",
                "${workspaceFolder}/components/libesphttpd/include"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "intelliSenseMode": "gcc-x64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "go2sh.cmake-integration"
        }
    ],
    "version": 4
}